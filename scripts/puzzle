set -e
path=`dirname $0`
cd ${path}
. minify-util
cd ..

solutionHtml=puzzle-solution.html
practiceHtml=puzzle-practice.html
srcDir=src/puzzle/
data=js/puzzle/data.js
pagesDir=pages/
minify ${pagesDir}${data} ${srcDir}${data}
minify ${pagesDir}${solutionHtml} ${srcDir}${solutionHtml}
minify ${pagesDir}${practiceHtml} ${srcDir}${practiceHtml}
